group_onlytime_sum <- function(input,output) {
library(dplyr)
library(tidyr)
data_1 <- read.csv(input)
traffic_data_sum <- data_1 %>% group_by(time) %>% summarise(sum=sum(sum, na.rm = TRUE)) %>% ungroup()
write.csv(traffic_data_sum,output)
}
