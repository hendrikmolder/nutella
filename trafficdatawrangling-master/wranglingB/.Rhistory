filter_notin <- function(input,output,colname,target) {
library(dplyr)
library(tidyr)
data_1 <- read.csv(input)
data_1 <- filter(data_1,!grepl(target, data_1[[colname]]))
write.csv(data_1,file=output)
#filter_in("g.csv","a.csv","day","2,9")
}
